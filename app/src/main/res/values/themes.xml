<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.CompassFree2021" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">#000000</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="SCBSwitch" parent="@style/Theme.AppCompat.Light">
        <item name="android:colorForeground">#A1A1A1</item>
        <item name="colorControlActivated">#ffffff</item>
        <item name="colorSwitchThumbNormal">#A1A1A1</item>
    </style>

    <style name="MinimalTransportControlsButton">
        <item name="android:background">@null</item>
        <item name="android:padding">@dimen/mcv2_pause_icon_padding</item>
        <item name="android:layout_width">@dimen/mcv2_icon_size</item>
        <item name="android:layout_height">@dimen/mcv2_icon_size</item>
        <item name="android:src">@drawable/ic_pause_circle_filled</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:contentDescription">@string/mcv2_pause_button_desc</item>
    </style>

    <style name="MoPubFullscreenTheme" parent="@style/Theme.AppCompat.Light">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="TitleBar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/mcv2_title_bar_height</item>
    </style>

    <style name="TitleBarButton">
        <item name="android:background">@null</item>
        <item name="android:padding">@dimen/mcv2_embedded_icon_padding</item>
        <item name="android:layout_width">@dimen/mcv2_icon_size</item>
        <item name="android:layout_height">@dimen/mcv2_icon_size</item>
        <item name="android:scaleType">fitXY</item>
    </style>

    <style name="TitleBarButton.Launch" parent="@style/TitleBarButton">
        <item name="android:src">@drawable/ic_launch</item>
        <item name="android:contentDescription">@string/mcv2_launch_button_desc</item>
    </style>

    <!--<style name="ratingbar" parent="@style/Theme.AppCompat">
        <item name="colorControlActivated">#f2c94c</item>
        <item name="colorControlNormal">#bcc2ce</item>
    </style>-->

    <style name="EmbeddedTransportControlsButton">
        <item name="android:background">@null</item>
        <item name="android:scaleType">fitXY</item>
    </style>

    <style name="EmbeddedTransportControlsButton.Ffwd" parent="@style/EmbeddedTransportControlsButton">
        <item name="android:padding">@dimen/mcv2_embedded_icon_padding</item>
        <item name="android:layout_width">@dimen/mcv2_icon_size</item>
        <item name="android:layout_height">@dimen/mcv2_icon_size</item>
        <item name="android:src">@drawable/ic_forward_30</item>
        <item name="android:contentDescription">@string/mcv2_ffwd_button_desc</item>
    </style>

    <style name="EmbeddedTransportControlsButton.Next" parent="@style/EmbeddedTransportControlsButton">
        <item name="android:padding">@dimen/mcv2_embedded_icon_padding</item>
        <item name="android:layout_width">@dimen/mcv2_icon_size</item>
        <item name="android:layout_height">@dimen/mcv2_icon_size</item>
        <item name="android:src">@drawable/ic_skip_next</item>
        <item name="android:contentDescription">@string/mcv2_next_button_desc</item>
