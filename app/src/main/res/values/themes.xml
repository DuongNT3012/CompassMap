    <style name="EmbeddedTransportControlsButton.Previous" parent="@style/EmbeddedTransportControlsButton">
        <item name="android:padding">@dimen/mcv2_embedded_icon_padding</item>
        <item name="android:layout_width">@dimen/mcv2_icon_size</item>
        <item name="android:layout_height">@dimen/mcv2_icon_size</item>
        <item name="android:src">@drawable/ic_skip_previous</item>
        <item name="android:contentDescription">@string/mcv2_previous_button_desc</item>
    </style>

    <style name="EmbeddedTransportControlsButton.Rew" parent="@style/EmbeddedTransportControlsButton">
        <item name="android:padding">@dimen/mcv2_embedded_icon_padding</item>
        <item name="android:layout_width">@dimen/mcv2_icon_size</item>
        <item name="android:layout_height">@dimen/mcv2_icon_size</item>
        <item name="android:src">@drawable/ic_rewind_10</item>
        <item name="android:contentDescription">@string/mcv2_rewind_button_desc</item>
    </style>

    <style name="FullTransportControlsButton">
        <item name="android:background">@null</item>
        <item name="android:scaleType">fitXY</item>
    </style>

    <style name="FullTransportControlsButton.Ffwd" parent="@style/FullTransportControlsButton">
        <item name="android:padding">@dimen/mcv2_full_icon_padding</item>
        <item name="android:layout_width">@dimen/mcv2_icon_size</item>
        <item name="android:layout_height">@dimen/mcv2_icon_size</item>
        <item name="android:src">@drawable/ic_forward_30</item>
        <item name="android:contentDescription">@string/mcv2_ffwd_button_desc</item>
    </style>

    <style name="FullTransportControlsButton.Next" parent="@style/FullTransportControlsButton">
        <item name="android:padding">@dimen/mcv2_full_icon_padding</item>
        <item name="android:layout_width">@dimen/mcv2_icon_size</item>
        <item name="android:layout_height">@dimen/mcv2_icon_size</item>
        <item name="android:src">@drawable/ic_skip_next</item>
        <item name="android:contentDescription">@string/mcv2_next_button_desc</item>
    </style>

    <style name="FullTransportControlsButton.Pause" parent="@style/FullTransportControlsButton">
        <item name="android:padding">@dimen/mcv2_pause_icon_padding</item>
        <item name="android:layout_width">@dimen/mcv2_icon_size</item>
        <item name="android:layout_height">@dimen/mcv2_icon_size</item>
        <item name="android:src">@drawable/ic_pause_circle_filled</item>
        <item name="android:contentDescription">@string/mcv2_pause_button_desc</item>
    </style>

    <style name="FullTransportControlsButton.Previous" parent="@style/FullTransportControlsButton">
        <item name="android:padding">@dimen/mcv2_full_icon_padding</item>
        <item name="android:layout_width">@dimen/mcv2_icon_size</item>
        <item name="android:layout_height">@dimen/mcv2_icon_size</item>
        <item name="android:src">@drawable/ic_skip_previous</item>
        <item name="android:contentDescription">@string/mcv2_previous_button_desc</item>
    </style>

    <style name="FullTransportControlsButton.Rew" parent="@style/FullTransportControlsButton">
        <item name="android:padding">@dimen/mcv2_full_icon_padding</item>
        <item name="android:layout_width">@dimen/mcv2_icon_size</item>
        <item name="android:layout_height">@dimen/mcv2_icon_size</item>
        <item name="android:src">@drawable/ic_rewind_10</item>
        <item name="android:contentDescription">@string/mcv2_rewind_button_desc</item>
    </style>

    <style name="BottomBarButton">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:background">@null</item>
        <item name="android:padding">@dimen/mcv2_embedded_icon_padding</item>
        <item name="android:layout_width">@dimen/mcv2_icon_size</item>
        <item name="android:layout_height">@dimen/mcv2_icon_size</item>
        <item name="android:scaleType">fitXY</item>
    </style>

    <style name="BottomBarButton.CC" parent="@style/BottomBarButton">
        <item name="android:src">@drawable/ic_subtitle_off</item>
        <item name="android:contentDescription">@string/mcv2_cc_is_off</item>
    </style>

    <style name="BottomBarButton.FullScreen" parent="@style/BottomBarButton">
        <item name="android:src">@drawable/ic_fullscreen</item>
        <item name="android:contentDescription">@string/mcv2_full_screen_button_desc</item>
    </style>

    <style name="BottomBarButton.OverflowHide" parent="@style/BottomBarButton">
        <item name="android:src">@drawable/ic_chevron_left</item>
        <item name="android:contentDescription">@string/mcv2_overflow_left_button_desc</item>
    </style>

    <style name="BottomBarButton.OverflowShow" parent="@style/BottomBarButton">
        <item name="android:src">@drawable/ic_chevron_right</item>
        <item name="android:contentDescription">@string/mcv2_overflow_right_button_desc</item>
    </style>

    <style name="BottomBarButton.Settings" parent="@style/BottomBarButton">
        <item name="android:src">@drawable/ic_settings</item>
        <item name="android:contentDescription">@string/mcv2_settings_button_desc</item>
    </style>

    <style name="TimeText">
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">left|center_horizontal|clip_horizontal</item>
        <item name="android:layout_gravity">clip_horizontal</item>
        <item name="android:paddingLeft">4dp</item>
        <item name="android:paddingRight">4dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="TimeText.Current" parent="@style/TimeText">
        <item name="android:textColor">@color/white</item>
        <item name="android:text">@string/MediaControlView_time_placeholder</item>
    </style>

    <style name="TimeText.End" parent="@style/TimeText">
        <item name="android:textColor">@color/white_opacity_70</item>
        <item name="android:text">@string/MediaControlView_time_placeholder</item>
    </style>

    <style name="TimeText.Interpunct" parent="@style/TimeText">
        <item name="android:textColor">@color/white_opacity_70</item>
        <item name="android:text">·</item>
    </style>

    <style name="RecoveryTheme" parent="@style/Theme.AppCompat.Light.DarkActionBar">
        <item name="android:windowBackground">@null</item>
        <item name="colorAccent">@color/recovery_colorAccent</item>
        <item name="colorPrimary">@color/recovery_colorPrimary</item>
        <item name="colorPrimaryDark">@color/recovery_colorPrimaryDark</item>
    </style>

    <style name="RecoveryTheme.AppBarOverlay" parent="@style/ThemeOverlay.AppCompat.Dark.ActionBar"></style>

    <style name="RecoveryTheme.NoActionBar" parent="@style/RecoveryTheme">

        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="RecoveryTheme.PopupOverlay" parent="@style/ThemeOverlay.AppCompat.Light"></style>

</resources>